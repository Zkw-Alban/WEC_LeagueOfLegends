t":[100,100,100],"costBurn":"100","datavalues":{},"effect":[null,[100,150,200],[300,450,600],[3,3,3],[700,700,700],[875,875,875],[875,875,875],[1200,1200,1200],[10,10,10],[0.5,0.5,0.5],[0,0,0]],"effectBurn":[null,"100/150/200","300/450/600","3","700","875","875","1200","10","0.5","0"],"vars":[],"costType":" {{ abilityresourcename }}","maxammo":"-1","range":[725,725,725],"rangeBurn":"725","image":{"full":"ReapTheWhirlwind.png","sprite":"spell4.png","group":"spell","x":240,"y":144,"w":48,"h":48},"resource":"{{ cost }} {{ abilityresourcename }}"}],"passive":{"name":"順風而行","description":"珍娜獲得額外移動速度加成，且鄰近友方英雄朝她移動時將獲得相同移動速度加成。<br><br>此外，珍娜的普攻會根據她獲得的移動速度加成造成額外魔法傷害。","image":{"full":"Janna_Tailwind.png","sprite":"passive1.png","group":"passive","x":192,"y":48,"w":48,"h":48}},"recommended":[]}}}